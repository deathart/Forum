    <div class="forum-bloc">
        <div class="title">
            {% trans from 'forumshow'%}subforum{% endtrans %}
        </div>
        <div class="content">
            {% for forum in forum_child %}
            <ul>
                <li class="icon">
                    <i class="far fa-comments"></i>
                </li>
                <li class="title">
                    <a href="{{ url('forum_show', {'slug':forum.slug}) }}">{{ forum.name }}</a>
                    <p>{{ forum.desc }}</p>
                </li>
                <li class="stats d-none d-lg-block">
                    <p><strong>3</strong> {% trans from 'forumshow'%}count.topics{% endtrans %}</p>
                    <p><strong>23</strong> {% trans from 'forumshow'%}count.posts{% endtrans %}</p>
                </li>
                <li class="last-post">
                    <img src="https://dummyimage.com/75x75/000/fff.png" alt="deathart" class="rounded d-none d-lg-inline">
                    <span class="info">
                        <a href="#" class="recent-post">Global Topic in Information Câ€¦</a><br  />
                        <span class="recent-post-time">
                            {% trans with {'%name%': 'Deathart'} from 'home'%}lastpost.author{% endtrans %} Thu Jun 22, 2017 9:38 am
                        </span>
                    </span>
                </li>
            </ul>
            {% endfor %}
        </div>
    </div>
